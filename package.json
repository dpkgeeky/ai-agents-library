{
  "name": "ai-agents-library",
  "version": "0.1.0",
  "description": "A TypeScript library for creating AI agents with graph support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "example": "ts-node examples/basic-usage.ts"
  },
  "keywords": [
    "ai",
    "agents",
    "langchain",
    "langraph",
    "typescript",
    "graph",
    "llm",
    "ollama",
    "llama"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.1.0",
    "@langchain/langgraph": "^0.0.1",
    "@langchain/openai": "^0.0.14",
    "langchain": "^0.1.0",
    "zod": "^3.22.4",
    "ollama": "^0.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
